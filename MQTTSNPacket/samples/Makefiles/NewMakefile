TARGETS = qos0pub qos0pub_register

all: $(TARGETS)

qos0pub: *.c *.h
	$(CC) -Werror -Wall qos0pub.c transport.c -I ../src ../src/MQTTSNSerializePublish.c ../src/MQTTSNPacket.c ../src/MQTTSNConnectClient.c -o qos0pub -Os -s
	
qos0pub_register: *.c *.h
	$(CC) -Werror -Wall qos0pub_register.c transport.c -I ../src ../src/MQTTSNSerializePublish.c ../src/MQTTSNDeserializePublish.c ../src/MQTTSNPacket.c ../src/MQTTSNConnectClient.c -o qos0pub_register -Os -s
clean:
	rm -f $(TARGETS)

